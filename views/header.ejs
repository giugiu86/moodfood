            document.addEventListener("DOMContentLoaded", function() {
              const c_path_img = "<%= path_img %>"+"img_logo.png";
              var imgElement = document.getElementById("id_img_logo");
              if (imgElement) {
                imgElement.src = c_path_img;
              } else {
              console.error("Element with id 'path_img' not found.");
            }
          });
        </script>
      <!-- <img class="img_logo" src="images/img_logo.png" alt="Logo"> -->
